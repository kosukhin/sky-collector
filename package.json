{
  "name": "sky-collector",
  "version": "1.0.0",
  "description": "A multiplayer flying game where players can soar through the skies, collect items, and compete for the highest score",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "dev": "nodemon --exec ts-node src/server/index.ts",
    "dev-client": "webpack serve --mode development",
    "build": "npm run build-server && npm run build-client",
    "build-client": "webpack --mode production",
    "build-server": "tsc --project tsconfig.server.json",
    "build-dev": "webpack --mode development",
    "test": "jest",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "type-check-server": "tsc --project tsconfig.server.json --noEmit",
    "push-head": "git push origin HEAD",
    "commit": "cz",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "release:dry-run": "standard-version --dry-run"
  },
  "keywords": [
    "game",
    "multiplayer",
    "flying",
    "silentium",
    "nodejs",
    "collection",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "silentium": "^0.0.35",
    "silentium-components": "^0.0.20",
    "silentium-web-api": "^0.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@types/node": "^20.4.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.4.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^4.15.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "standard-version": "^9.5.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
